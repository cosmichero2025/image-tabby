{
    "name": "image-tabby",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Image-Tabby",
        "default_icon": "./popup/img/logo_48.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "./popup/img/logo_16.png",
        "48": "./popup/img/logo_48.png",
        "128": "./popup/img/logo_128.png"
    },
    "permissions": ["http://*/*", "tabs", "downloads"]
}